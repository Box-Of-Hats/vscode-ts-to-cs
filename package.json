{
    "name": "ts-to-csharp",
    "displayName": "ts-to-csharp",
    "author": {
        "name": "box-of-hats",
        "url": "https://github.com/Box-Of-Hats/vscode-ts-to-cs"
    },
    "repository": {
        "url": "https://github.com/Box-Of-Hats/vscode-ts-to-cs"
    },
    "publisher": "box-of-hats",
    "description": "Convert typescript to c#",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "./icon.png",
    "activationEvents": [
        "onCommand:extension.tscsharp.generateClassFromInterface",
        "onLanguage:typescriptreact",
        "onLanguage:typescript"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.tscsharp.generateClassFromInterface",
                "title": "Ts to Cs: Generate class from interface"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.38.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {
        "ts-csharp": "0.0.5"
    }
}
